{
    "remoteCommand": "/bin/bash",
    "args": [
      "-c",
      "echo 'hello world' && sleep 1"
    ],
    "minSlots": 1,
    "maxSlots": 1,
    "jobEnvironment": {
      "ENV_VAR_1": "value1",
      "ENV_VAR_2": "value2"
    },
    "errorPath": "/dev/stderr",
    "outputPath": "/dev/stdout",
    "queueName": "my-pubsub-topic",
    "extension": {
      "extensionList": {
        "googleProjectID": "my-project-id"
      }
    }
}